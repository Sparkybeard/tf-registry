{
  "name": "TFRegNodeTS",
  "version": "1.0.0",
  "license": "MIT",
  "type": "commonjs",
  "bin": {
    "TFRegNodeTS": "bin/TFRegNodeTS"
  },
  "scripts": {
    "lint": "eslint --ext .ts ./src",
    "start": "ts-node-dev --respawn --transpileOnly --ignore-watch node_modules --no-notify --exit-child src/main.ts",
    "build": "npm run build:linux && npm run build:macos && npm run build:windows",
    "build:linux": "pkg . --compress GZip --targets node16-linux-x64 -o dist/TFRegNodeTS-linux-x64",
    "build:macos": "pkg . --compress GZip --targets node16-macos-x64,node16-macos-arm64 -o dist/TFRegNodeTS-macos",
    "build:windows": "pkg . --compress GZip --targets node16-win-x64 -o dist/TFRegNodeTS-win-x64.exe",
    "dev": "nodemon",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:migrate": "prisma migrate dev --name init"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/mkdirp": "^2.0.0",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/multiparty": "^0.0.33",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/tar": "^6.1.4",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "concurrently": "^8.0.1",
    "eslint": "^8.37.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "prisma": "^4.12.0",
    "swagger-ui-express": "^4.6.2",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "tsoa": "^5.1.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "body-parser": "^1.20.2",
    "commander": "^10.0.0",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "got": "^12.6.0",
    "helmet": "^6.0.1",
    "jten": "^0.2.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multiparty": "^4.2.3",
    "ngrok": "^4.3.3",
    "ora": "^6.3.0",
    "pino": "^8.11.0",
    "pkg": "^5.8.1",
    "tar": "^6.1.13",
    "uuid": "^9.0.0",
    "uuidv4": "^6.2.13"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node main.ts"
  }
}
